{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false
    },
    {
      "name": "Debug: RDBMS Ingestion Pipeline",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/ingestion/rdbms_ingestion.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--ocs", "ecommerce_transactions_rdbms"
      ]
    },
    {
      "name": "Debug: FS Ingestion Pipeline",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/ingestion/csv_ingestion.py",
      "console": "integratedTerminal",
      // "preLaunchTask": "setting-classpath",
      "justMyCode": false,
      "args": [
        "--ocs", "ecommerce_transactions_fs"
      ]
    },
    {
      "name": "Debug: Streaming Ingestion Pipeline",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/ingestion/streaming_sub.py",
      "console": "integratedTerminal",
      "justMyCode": false,
      "args": [
        "--topic", "ecommerce_transactions_streaming",
        "--ocs", "ecommerce_transactions_streaming",
        "--batch-size", "1"
      ]
    },
    {
      "type": "java",
      "name": "Debug JUnit Test",
      "request": "launch",
      "mainClass": "",
      "projectName": "sentiment-anz-hadoop",
      "args": "",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "classPaths": ["${workspaceFolder}/resources/prod/src/test/java"],
      "modulePaths": []
    }
  ]
}
